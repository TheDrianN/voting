generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Votes{
  id Int @id @default(autoincrement())
  sub_election_id Int
  user_id Int
  group_candidates_id Int?
  vote_type String  @db.VarChar(5)
  date_vote DateTime @default(now()) @db.Timestamp(0)
  updated_at DateTime @updatedAt @db.Timestamp(0)
}